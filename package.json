{
  "name": "react-realworld-example",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=\"development\" run-s sprite:svg _webpack:dev",
    "debug": "cross-env DEBUG_ENV=\"enable\" run-s sprite:svg _webpack:dev",
    "build": "cross-env NODE_ENV=\"production\" run-s sprite:svg del:dist copy:static _webpack:production",
    "commit": "commit",
    "lint:ts": "eslint --fix \"src/**/*.ts{,x}\"",
    "lint:css": "stylelint \"**/*.ts{,x}\" --fix",
    "fmt": "prettier --loglevel warn --write \"**/*.{js,ts,tsx}\"",
    "test": "jest",
    "test:coverage": "jest --ci --runInBand --coverage && codecov",
    "test:watch": "jest --watch",
    "tsc": "tsc --noEmit",
    "e2e": "cypress open",
    "e2e:ci": "cypress run",
    "e2e:update": "cypress open --env updateSnapshots=true",
    "typecheck": "tsc --noEmit",
    "storybook": "start-storybook -p 6006 -s ./src/static",
    "storybook:build": "build-storybook",
    "storycap": "rm -rf ./__screenshots__ && storycap --serverCmd \"yarn storybook --ci\" http://localhost:6006 -f \"true\" --serverTimeout \"200000\"",
    "regression": "reg-suit run",
    "copy:static": "cpx \"./src/static/**/*\" \"./dist\"",
    "del:dist": "rm -rf \"./dist\"",
    "sprite:svg": "svg-sprite-generate -d ./material/sprite -o ./src/static/img/icon/icons.svg",
    "_webpack:dev": "webpack-dev-server --config webpack.dev.js",
    "_webpack:production": "webpack --config webpack.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryu-f/react-realworld-example.git"
  },
